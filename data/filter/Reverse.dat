program Reverse
  M1 <- 21845  {- 0101010101010101 -}
  M2 <- 13107  {- 0011001100110011 -}
  M3 <- 3855   {- 0000111100001111 -}
  M4 <- 255    {- 0000000011111111 -}

  k <- 0
  while k < 16 do
     x <- RECEIVE[0]
     x <- ((x & M1) << 1) | ((x >> 1) & M1)
     x <- ((x & M2) << 2) | ((x >> 2) & M2)
     x <- ((x & M3) << 4) | ((x >> 4) & M3)
     x <- ((x & M4) << 8) | ((x >> 8) & M4)
     SEND x
     k <- k + 1
   end-while
end-program
